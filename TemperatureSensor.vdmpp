class TemperatureSensor is subclass of Sensor

operations

public TemperatureSensor: nat * sensorType * nat ==> TemperatureSensor
	TemperatureSensor (id, type, val) ==
		(ID := id;
		Type := type;
		Value := val;
		);

public Step: () ==> ()
	Step() ==
		(Value := World`env.ReadTemp();
		);


end TemperatureSensor